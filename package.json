{
  "name": "client",
  "version": "0.0.1",
  "description": "Linting project",
  "main": "index.js",
  "scripts": {
    "lint": "eslint \"**/*.{js,jsx}\" --ignore-path .eslintignore",
    "lint-debug": "eslint \"**/*.{js,jsx}\" --ignore-path .eslintignore --debug",
    "lint-fix": "eslint \"**/*.{js,jsx}\" --ignore-path .eslintignore --fix",
    "style-lint": "stylelint \"**/*.{scss,css}\" --ignore-path .eslintignore",
    "style-lint-fix": "stylelint \"**/*.{scss,css}\" --ignore-path .eslintignore --fix",
    "start": "webpack-dev-server --mode development --open --history-api-fallback --hot --inline --config webpack.dev.js",
    "build": "webpack --mode production --config webpack.prod.js"
  },
  "author": "Izabella Szlosarczyk",
  "keywords": [
    "linting",
    "react-linting",
    "style-linting"
  ],
  "dependencies": {
    "dotenv": "^8.2.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-optional-chaining": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.1",
    "@babel/preset-react": "^7.10.1",
    "@hot-loader/react-dom": "^16.13.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-react-css-modules": "^2.8.0",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "file-loader": "^5.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "react-hot-loader": "^4.12.21",
    "sass-loader": "^8.0.2",
    "sass-resources-loader": "^2.0.3",
    "style-loader": "^1.2.1",
    "stylelint": "^13.9.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-csstree-validator": "^1.9.0",
    "stylelint-scss": "^3.18.0",
    "terser-webpack-plugin": "^3.0.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
